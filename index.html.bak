    <!-- Critical CSS inlined -->
    <style>
        /* Critical CSS for above-the-fold content */
        * {margin: 0; padding: 0; box-sizing: border-box;}
        :root {
            --accent: #22C55E;
            --font-primary: 'Inter', -apple-system, BlinkMacSystemFont, 'SF Pro Display', 'Segoe UI', Roboto, sans-serif;
            --font-size-base: 16px;
            --font-size-h1: clamp(2rem, 5vw + 1rem, 3.5rem);
            --font-size-h2: clamp(1.5rem, 3vw + 0.5rem, 2.5rem);
            --font-size-h3: clamp(1.2rem, 2vw + 0.5rem, 1.8rem);
            --font-size-subtitle: clamp(1rem, 1.5vw + 0.5rem, 1.3rem);
        }
        body {
            font-family: var(--font-primary);
            font-size: var(--font-size-base);
            line-height: 1.5;
            color: #1A202C;
            background-color: #FFFFFF;
            opacity: 1;
            transition: opacity 0.3s ease;
            -webkit-text-size-adjust: 100%; /* Prevent iOS font size adjustments */
        }
        body.loaded {opacity: 1;}
        .hero {
            min-height: 85vh;
            display: flex;
            align-items: center;
            justify-content: center;
            color: #fff;
            position: relative;
            overflow: hidden;
            background: #000;
            padding-top: 80px;
            padding-bottom: 30px;
            margin-bottom: -50px;
            contain: layout paint; /* Optimize rendering performance */
        }
        .hero-content {
            max-width: 1000px;
            margin: 0 auto;
            position: relative;
            z-index: 2;
            text-align: center;
            padding: 0 2rem;
            will-change: transform; /* Hint for browser optimization */
        }
        
        /* Hero background elements - fix ghosting */
        .hero-image-container {
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            overflow: hidden;
            background-color: #000; /* Ensure black background */
        }
        
        .hero-overlay {
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            background: rgba(0, 0, 0, 0.5); /* Semi-transparent overlay */
            z-index: 1;
        }
        
        .hero-fallback-img {
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            object-fit: cover;
            transform: translateZ(0); /* Force GPU acceleration */
            backface-visibility: hidden; /* Prevent ghosting */
            will-change: opacity; /* Hint for browser optimization */
        }
        
        .hero-video {
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            object-fit: cover;
            z-index: 0;
            transform: translateZ(0); /* Force GPU acceleration */
            backface-visibility: hidden; /* Prevent ghosting */
            will-change: transform; /* Hint for browser optimization */
        }
        
        /* Hide picture when video plays */
        .hero-video:playing + picture {
            display: none;
        }
        
        /* For browsers that don't support :playing */
        .video-active picture {
            display: none;
        }
        
        /* Hero image container adjustments for various browsers */
        @supports (-webkit-overflow-scrolling: touch) {
            /* iOS-specific adjustments */
            .hero-video {
                position: absolute;
                width: 100%;
                height: 100%;
                object-fit: cover;
                z-index: 0;
            }
        }
        
        /* Enhanced SVG icon styling */
        .stat-icon {
            display: flex;
            align-items: center;
            justify-content: center;
            width: 48px;
            height: 48px;
            border-radius: 12px;
            background: rgba(34, 197, 94, 0.1);
            margin-bottom: 1rem;
            transition: all 0.3s ease;
        }
        
        .stat-card:hover .stat-icon {
            transform: scale(1.1);
            background: rgba(34, 197, 94, 0.2);
            box-shadow: 0 0 15px rgba(34, 197, 94, 0.3);
        }
        
        .stat-icon svg {
            width: 24px;
            height: 24px;
            fill: var(--accent);
            transition: all 0.3s ease;
        }
        
        .stat-card:hover .stat-icon svg {
            fill: var(--accent);
            filter: drop-shadow(0 0 3px rgba(34, 197, 94, 0.5));
        }
        
        /* Enhanced partner logo styling */
        .partner-image {
            position: relative;
            overflow: hidden;
            border-radius: 12px;
            transition: all 0.3s ease;
            box-shadow: 0 8px 20px rgba(0, 0, 0, 0.1);
        }
        
        .partner-image::before {
            content: '';
            position: absolute;
            top: 0;
            left: -100%;
            width: 100%;
            height: 100%;
            background: linear-gradient(90deg, transparent, rgba(255, 255, 255, 0.2), transparent);
            transition: left 0.8s ease;
            z-index: 1;
        }
        
        .partner-card:hover .partner-image::before {
            left: 100%;
        }
        
        .partner-image img {
            transition: transform 0.5s ease;
            display: block;
            max-width: 100%;
        }
        
        .partner-card:hover .partner-image img {
            transform: scale(1.05);
        }
        
        .partner-info h3 {
            font-weight: 700;
            transition: color 0.3s ease;
        }
        
        .partner-card:hover .partner-info h3 {
            color: var(--accent);
        }
        
        /* Enhanced main logo styling */
        .logo-text {
            color: #fff;
            font-size: 2rem;
            font-weight: 800;
            letter-spacing: -0.02em;
            position: relative;
            display: inline-block;
        }
        
        .logo-text::after {
            content: '';
            position: absolute;
            bottom: -3px;
            left: 0;
            width: 0;
            height: 2px;
            background: var(--accent);
            transition: width 0.3s ease;
        }
        
        .nav-logo:hover .logo-text::after {
            width: 100%;
        }
        
        /* Enhanced Responsive Design CSS */
        /* Fluid Typography Using clamp() */
        h1 {
            font-size: var(--font-size-h1);
            line-height: 1.2;
        }
        
        h2 {
            font-size: var(--font-size-h2);
            line-height: 1.3;
        }
        
        h3 {
            font-size: var(--font-size-h3);
            line-height: 1.4;
        }
        
        .subheadline, 
        .partners-description, 
        .signup-description {
            font-size: var(--font-size-subtitle);
        }
        
        /* Tablet Portrait Breakpoint */
        @media (max-width: 992px) {
            .hero-content {
                max-width: 90%;
            }
            .share-grid, 
            .benefits-grid, 
            .stats-grid {
                grid-template-columns: repeat(2, 1fr);
                gap: 1.5rem;
            }
            .two-column-grid {
                grid-template-columns: 1fr;
                gap: 2rem;
            }
            .partners-scroll {
                max-width: 90%;
            }
        }
        
        /* Mobile Landscape Breakpoint */
        @media (max-width: 768px) {
            .hero {
                min-height: 80vh;
                contain: layout paint; /* Optimize rendering on mobile */
            }
            .hero-video {
                display: none;
            }
            .hero-image-container {
                background-image: url('https://i.imgur.com/lhhxgxB.jpg');
                background-size: cover;
                background-position: center;
                contain: strict; /* Further optimize rendering */
            }
            .navbar {
                display: flex;
                justify-content: space-between;
                padding: 10px 20px;
                background-color: #000;
            }
            .navbar .nav-logo {font-size: 1.2rem;}
            .navbar .nav-container {
                flex-direction: column;
                align-items: center;
            }
            /* Optimize tap targets for mobile */
            button, 
            [role="button"],
            a {
                min-height: 44px;
                min-width: 44px;
            }
            /* Prevent content reflow */
            img, video {
                height: auto;
                aspect-ratio: attr(width) / attr(height);
            }
        }
        
        /* Small Mobile Breakpoint */
        @media (max-width: 480px) {
            h1 {
                font-size: 2rem;
                line-height: 1.2;
            }
            h2 {
                font-size: 1.5rem;
            }
            .hero-stats {
                flex-direction: column;
                gap: 1rem;
            }
            .share-grid, 
            .benefits-grid, 
            .stats-grid {
                grid-template-columns: 1fr;
            }
            .footer-content {
                flex-direction: column;
                text-align: center;
            }
            .partner-card {
                flex: 0 0 90%;
            }
        }
        
        /* Support for High-DPI screens */
        @media (-webkit-min-device-pixel-ratio: 2), (min-resolution: 192dpi) {
            .hero-image-container {
                background-image: url('https://i.imgur.com/lhhxgxB@2x.jpg');
            }
        }
        
        /* Respect User's Motion Preference */
        @media (prefers-reduced-motion: reduce) {
            * {
                animation-duration: 0.01ms !important;
                animation-iteration-count: 1 !important;
                transition-duration: 0.01ms !important;
                scroll-behavior: auto !important;
            }
        }
        
        /* Enhanced Accessibility Focus Styles */
        @media (max-width: 768px) {
            /* Larger focus indicators on mobile for touch targets */
            a:focus,
            button:focus,
            [role="button"]:focus,
            input:focus,
            select:focus,
            textarea:focus {
                outline: 3px solid var(--accent) !important;
                outline-offset: 3px !important;
            }
        }
        
        /* High contrast mode support */
        @media (forced-colors: active) {
            /* Ensure buttons and interactive elements have clear boundaries */
            button, 
            [role="button"],
            a,
            .benefit-card,
            .partner-card,
            .share-card {
                border: 1px solid;
            }
            
            /* Ensure icons are visible */
            svg {
                forced-color-adjust: none;
            }
        }
        
        /* Container query polyfill styles */
        /* For browsers that don't support container queries */
        @supports not (container-type: inline-size) {
            .benefit-card,
            .partner-card,
            .stat-card {
                max-width: 100%;
            }
            
            @media (min-width: 768px) {
                .benefit-card,
                .partner-card,
                .stat-card {
                    max-width: 350px;
                }
            }
        }
        
        /* For browsers that support container queries */
        @supports (container-type: inline-size) {
            .benefits-grid,
            .partners-track,
            .stats-grid {
                container-type: inline-size;
            }
            
            /* Container queries */
            @container (min-width: 600px) {
                .benefit-card {
                    display: flex;
                    align-items: center;
                }
                
                .benefit-icon {
                    margin-right: 1rem;
                    margin-bottom: 0;
                }
            }
        }
        
        /* Modern CSS aspect ratio instead of HTML attributes */
        @supports (aspect-ratio: 16 / 9) {
            .hero-video, 
            .hero-fallback-img {
                aspect-ratio: 16 / 9;
                height: auto;
                width: 100%;
            }
            
            .partner-image img {
                aspect-ratio: 1 / 1;
                height: auto;
                width: 100%;
            }
            
            .share-card img {
                aspect-ratio: 3 / 2;
                height: auto;
                width: 100%;
            }
        }
        
        /* Print Media Styles - Important for accessibility and better user experience */
        @media print {
            body {
                font-size: 12pt;
                line-height: 1.5;
                color: #000;
                background: #fff;
            }
            
            nav, 
            footer, 
            video, 
            button,
            .mobile-cta,
            .partners-scroll,
            .exit-popup,
            .exit-popup-overlay {
                display: none !important;
            }
            
            a {
                text-decoration: underline;
                color: #000;
            }
            
            a[href]:after {
                content: " (" attr(href) ")";
                font-size: 0.8em;
            }
            
            h1, h2, h3, h4, h5, h6 {
                page-break-after: avoid;
                page-break-inside: avoid;
            }
            
            img {
                max-width: 100% !important;
                page-break-inside: avoid;
            }
            
            .share-card,
            .benefit-card,
            .stat-card {
                break-inside: avoid;
                page-break-inside: avoid;
            }
            
            .hero {
                height: auto;
                min-height: 0;
                padding: 1cm 0;
                background: none;
                color: #000;
            }
            
            .hero-overlay,
            .hero-video,
            .hero-image-container picture {
                display: none;
            }
            
            .section-transition {
                background: none !important;
                color: #000 !important;
                margin: 1cm 0;
                padding: 0;
            }
            
            /* Ensure URL display for important links */
            .partner-card a[href]:after,
            .benefit-card a[href]:after {
                display: block;
                font-size: 0.8em;
            }
        }
    </style>

