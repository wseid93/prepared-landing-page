    <!-- Consolidated and optimized JS -->
    <script defer>
        // Initialize only critical functionality immediately
        document.body.classList.add('js-loading');
        
        // Performance monitoring
        if (window.performance && window.performance.timing) {
            window.addEventListener('load', function() {
                setTimeout(function() {
                    const timing = window.performance.timing;
                    const pageLoadTime = timing.loadEventEnd - timing.navigationStart;
                    const domLoadTime = timing.domContentLoadedEventEnd - timing.navigationStart;
                    
                    console.log('Page Load Time: ' + (pageLoadTime / 1000).toFixed(2) + ' seconds');
                    console.log('DOM Load Time: ' + (domLoadTime / 1000).toFixed(2) + ' seconds');
                    
                    // Add to localStorage for comparison with future visits
                    try {
                        const prevLoadTime = localStorage.getItem('prevPageLoadTime');
                        if (prevLoadTime) {
                            const improvement = ((prevLoadTime - pageLoadTime) / prevLoadTime * 100).toFixed(1);
                            if (improvement > 0) {
                                console.log('Performance improved by ' + improvement + '% since last visit');
                            }
                        }
                        localStorage.setItem('prevPageLoadTime', pageLoadTime);
                    } catch (e) {
                        // Ignore storage errors
                    }
                }, 0);
            });
        }
        
        // Defer non-critical functionality
        window.addEventListener('DOMContentLoaded', () => {
            // Fix hero video ghosting effect
            const fixHeroVideoGhosting = () => {
                const heroVideo = document.querySelector('.hero-video');
                const heroPicture = document.querySelector('.hero-image-container picture');
                
                if (heroVideo && heroPicture) {
                    // Check if video can play
                    const canPlayVideo = !(/iPhone|iPad|iPod/.test(navigator.userAgent)) || 
                                        (document.cookie.indexOf('allowAutoplay=true') !== -1);
                    
                    if (canPlayVideo) {
                        // Handle video playing state
                        heroVideo.addEventListener('playing', () => {
                            // When video plays, hide the picture element
                            heroPicture.style.display = 'none';
                        });
                        
                        // Ensure picture shows if video fails to play
                        heroVideo.addEventListener('error', () => {
                            heroPicture.style.display = 'block';
                            heroVideo.style.display = 'none';
                        });
                        
                        // Check if video is already playing (in case event was missed)
                        if (!heroVideo.paused && heroVideo.currentTime > 0) {
                            heroPicture.style.display = 'none';
                        }
                    } else {
                        // For devices that won't autoplay video, hide video and show picture
                        heroVideo.style.display = 'none';
                        heroPicture.style.display = 'block';
                    }
                }
            };
            
            // Call the fix
            fixHeroVideoGhosting();
            
            // Responsive image loading optimization
            const loadResponsiveImages = () => {
                // Check if the browser supports IntersectionObserver
                if ('IntersectionObserver' in window) {
                    const imageObserver = new IntersectionObserver((entries) => {
                        entries.forEach(entry => {
                            if (entry.isIntersecting) {
                                const img = entry.target;
                                // Check for data-srcset attribute and apply it to srcset
                                if (img.dataset.srcset) {
                                    img.srcset = img.dataset.srcset;
                                }
                                // Check for data-src attribute and apply it to src
                                if (img.dataset.src) {
                                    img.src = img.dataset.src;
                                }
                                img.classList.add('loaded');
                                imageObserver.unobserve(img);
                            }
                        });
                    }, { rootMargin: '50px 0px', threshold: 0.01 });
                    
                    // Target all images with data-src attribute
                    document.querySelectorAll('img[data-src]').forEach(img => {
                        imageObserver.observe(img);
                    });
                } else {
                    // Fallback for browsers without IntersectionObserver
                    document.querySelectorAll('img[data-src]').forEach(img => {
                        img.src = img.dataset.src;
                        if (img.dataset.srcset) {
                            img.srcset = img.dataset.srcset;
                        }
                        img.classList.add('loaded');
                    });
                }
            };
            
            // Call the function
            loadResponsiveImages();
            
            // Add support for browsers that don't support native lazy loading
            if (!('loading' in HTMLImageElement.prototype)) {
                // If native lazy loading isn't supported, implement the IntersectionObserver-based solution
                const lazyImages = document.querySelectorAll('img[loading="lazy"]');
                
                if ('IntersectionObserver' in window) {
                    const lazyImageObserver = new IntersectionObserver((entries) => {
                        entries.forEach(entry => {
                            if (entry.isIntersecting) {
                                const lazyImage = entry.target;
                                lazyImage.src = lazyImage.dataset.src || lazyImage.src;
                                if (lazyImage.dataset.srcset) {
                                    lazyImage.srcset = lazyImage.dataset.srcset;
                                }
                                lazyImage.classList.add('loaded');
                                lazyImageObserver.unobserve(lazyImage);
                            }
                        });
                    });
                    
                    lazyImages.forEach(lazyImage => {
                        // Store original src in dataset if not already there
                        if (!lazyImage.dataset.src && lazyImage.src) {
                            lazyImage.dataset.src = lazyImage.src;
                            // Temporarily use a tiny transparent placeholder
                            lazyImage.src = 'data:image/svg+xml,%3Csvg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 1 1"%3E%3C/svg%3E';
                        }
                        
                        lazyImageObserver.observe(lazyImage);
                    });
                }
            }
            
            // Improve accessibility for touch devices
            if ('ontouchstart' in window || navigator.maxTouchPoints > 0) {
                document.body.classList.add('touch-device');
                
                // Enhance tap targets on touch devices
                const smallTapTargets = document.querySelectorAll('.partner-title, .checklist-header, .stat-label');
                smallTapTargets.forEach(target => {
                    target.style.minHeight = '44px';
                    target.style.lineHeight = '44px';
                });
            }
            
            // Detect screen size changes and update UI accordingly
            const handleResponsiveChanges = () => {
                const isMobile = window.matchMedia('(max-width: 768px)').matches;
                const isTablet = window.matchMedia('(min-width: 769px) and (max-width: 992px)').matches;
                
                // Add appropriate classes to body
                document.body.classList.toggle('is-mobile', isMobile);
                document.body.classList.toggle('is-tablet', isTablet);
                
                // Adjust video loading based on connection and device
                const connectionType = navigator.connection ? navigator.connection.effectiveType : '4g';
                const heroVideo = document.querySelector('.hero-video');
                const heroContainer = document.querySelector('.hero-image-container');
                
                if (heroVideo && (isMobile || connectionType === 'slow-2g' || connectionType === '2g')) {
                    // Prevent video loading on mobile or slow connections
                    heroVideo.setAttribute('preload', 'none');
                    heroVideo.pause();
                    heroVideo.removeAttribute('autoplay');
                    heroContainer?.classList.remove('video-active');
                } else if (heroVideo) {
                    heroVideo.setAttribute('preload', 'metadata');
                    // Add class when video plays to fix ghosting
                    heroVideo.addEventListener('playing', () => {
                        heroContainer?.classList.add('video-active');
                    });
                }
            };
            
            // Call responsive handler initially
            handleResponsiveChanges();
            
            // Listen for window resize events with debounce
            let resizeTimeout;
            window.addEventListener('resize', () => {
                clearTimeout(resizeTimeout);
                resizeTimeout = setTimeout(handleResponsiveChanges, 100);
            }, { passive: true });
            
            // Exit Intent handling
            let hasShownPopup = false;
            document.addEventListener('mouseleave', (e) => {
                if (e.clientY < 0 && !hasShownPopup) {
                    document.getElementById('exit-popup').classList.add('visible');
                    document.getElementById('exit-popup-overlay').classList.add('visible');
                    hasShownPopup = true;
                }
            });

            document.getElementById('exit-popup-overlay')?.addEventListener('click', () => {
                document.getElementById('exit-popup').classList.remove('visible');
                document.getElementById('exit-popup-overlay').classList.remove('visible');
            });
            
            // Update spots counter functionality
            const updateSpotsDisplay = (spots) => {
                const spotCounters = document.querySelectorAll('#spots-left, #popup-spots-left');
                spotCounters.forEach(counter => {
                    if (counter) counter.textContent = spots;
                });

                if (spots <= 0) {
                    document.querySelectorAll('.spots-counter').forEach(el => {
                        if (el) el.style.display = 'none';
                    });
                }
            };
            
            let spotsLeft = 85; // Initial spots
            updateSpotsDisplay(spotsLeft);

            // Listen for Typeform submission events
            window.addEventListener('message', (event) => {
                if (event.data.type === 'form-submit') {
                    spotsLeft = Math.max(0, spotsLeft - 1);
                    updateSpotsDisplay(spotsLeft);
                }
            });
            
            // Initialize countdown timer with requestAnimationFrame for better performance
            const updateCountdown = () => {
                const endDate = new Date('2025-04-12T06:59:59Z').getTime();
                
                const updateTimer = () => {
                    const currentTime = new Date().getTime();
                    const difference = endDate - currentTime;
                    
                    const days = Math.floor(difference / (1000 * 60 * 60 * 24));
                    const hours = Math.floor((difference % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60));
                    const minutes = Math.floor((difference % (1000 * 60 * 60)) / (1000 * 60));
                    const seconds = Math.floor((difference % (1000 * 60)) / 1000);
                    
                    const daysElement = document.getElementById('countdown-days');
                    const hoursElement = document.getElementById('countdown-hours');
                    const minutesElement = document.getElementById('countdown-minutes');
                    const secondsElement = document.getElementById('countdown-seconds');
                    
                    if (daysElement) daysElement.textContent = String(days).padStart(2, '0');
                    if (hoursElement) hoursElement.textContent = String(hours).padStart(2, '0');
                    if (minutesElement) minutesElement.textContent = String(minutes).padStart(2, '0');
                    if (secondsElement) secondsElement.textContent = String(seconds).padStart(2, '0');
                    
                    // Use requestAnimationFrame instead of setInterval for better performance
                    if (difference > 0) {
                        // Only update once per second to save CPU on mobile
                        setTimeout(() => requestAnimationFrame(updateTimer), 1000);
                    }
                }
                
                updateTimer();
            };
            
            // Initialize countdown if elements exist
            if (document.getElementById('countdown-days')) {
                updateCountdown();
            }
        });
        
        // Use window load event for non-critical visual enhancements
        window.addEventListener('load', () => {
            document.body.classList.add('loaded');
            
            // Initialize Intersection Observer for animations
            if ('IntersectionObserver' in window) {
                const animationObserver = new IntersectionObserver((entries) => {
                    entries.forEach(entry => {
                        if (entry.isIntersecting) {
                            entry.target.classList.add('animate-visible');
                            
                            // Add card index for staggered animations
                            const cards = entry.target.querySelectorAll('.benefit-card, .testimonial-card');
                            cards.forEach((card, index) => {
                                card.style.setProperty('--card-index', index);
                            });
                        }
                    });
                }, { 
                    root: null, 
                    rootMargin: '0px', 
                    threshold: 0.1  // Lower threshold to improve performance
                });

                // Detect if device is low-end
                const isLowEndDevice = () => {
                    // Check for low memory (less than 2GB)
                    if (navigator.deviceMemory && navigator.deviceMemory < 2) return true;
                    
                    // Check for low-end CPU (older/slower devices)
                    if (navigator.hardwareConcurrency && navigator.hardwareConcurrency <= 4) return true;
                    
                    return false;
                };
                
                // Only enable animations on capable devices
                if (!isLowEndDevice() && !window.matchMedia('(prefers-reduced-motion: reduce)').matches) {
                    // Observe all animated sections
                    document.querySelectorAll('.section-transition, .testimonials, .trusted-by').forEach((element) => {
                        animationObserver.observe(element);
                    });
                } else {
                    // For low-end devices, just show everything without animations
                    document.querySelectorAll('.section-transition, .testimonials, .trusted-by').forEach((element) => {
                        element.classList.add('animate-visible');
                    });
                }
            } else {
                // Fallback for browsers that don't support IntersectionObserver
                document.querySelectorAll('.section-transition, .testimonials, .trusted-by').forEach((element) => {
                    element.classList.add('animate-visible');
                });
            }

            // Parallax scroll effect for hero section (optimized with requestAnimationFrame)
            let lastScrollY = window.scrollY;
            const hero = document.querySelector('.hero');
            let ticking = false;
            
            // Only enable parallax on desktop devices
            const isMobile = window.matchMedia('(max-width: 768px)').matches;
            
            if (!isMobile && hero) {
                window.addEventListener('scroll', () => {
                    if (!ticking) {
                        requestAnimationFrame(() => {
                            const scrollY = window.scrollY;
                            if (scrollY > 0 && scrollY < window.innerHeight) {
                                const offset = scrollY * 0.5;
                                hero.style.setProperty('--scroll-offset', `${offset}px`);
                                hero.classList.add('scrolling');
                            } else {
                                hero.classList.remove('scrolling');
                            }
                            lastScrollY = scrollY;
                            ticking = false;
                        });
                        ticking = true;
                    }
                }, {passive: true});
            }

            // Mobile CTA visibility optimized with passive event listener
            const mobileCTA = document.querySelector('.mobile-cta');
            let lastScrollTop = 0;
            let ctaTicking = false;
            
            if (mobileCTA) {
                // Use passive event listener for better scroll performance
                window.addEventListener('scroll', () => {
                    if (!ctaTicking) {
                        requestAnimationFrame(() => {
                            const scrollTop = window.pageYOffset || document.documentElement.scrollTop;
                            const isScrollingDown = scrollTop > lastScrollTop;
                            const isTopOfPage = scrollTop < 100;

                            if (isScrollingDown && !isTopOfPage) {
                                mobileCTA.classList.add('mobile-cta-visible');
                            } else {
                                mobileCTA.classList.remove('mobile-cta-visible');
                            }

                            lastScrollTop = scrollTop;
                            ctaTicking = false;
                        });
                        ctaTicking = true;
                    }
                }, { passive: true }); // Add passive flag for better performance
            }
                            
            // Smooth scroll for navigation links
            document.querySelectorAll('a[href^="#"]').forEach(anchor => {
                anchor.addEventListener('click', function (e) {
                    e.preventDefault();
                    const target = document.querySelector(this.getAttribute('href'));
                    if (target) {
                        const headerOffset = 80;
                        const elementPosition = target.getBoundingClientRect().top;
                        const offsetPosition = elementPosition - headerOffset;

                        window.scrollBy({
                            top: offsetPosition,
                            behavior: 'smooth'
                        });
                    }
                });
            });
        });
    </script>
    
    <!-- Typeform initialization script moved from head for performance -->
    <script defer>
        // Initialize Typeform after page load
        window.addEventListener('load', () => {
            document.body.classList.add('loaded');
            
            // Make sure Typeform elements are initialized properly
            if (window.tf && typeof window.tf.createWidget === 'function') {
                const widgets = document.querySelectorAll('[data-tf-live]');
                if (widgets.length > 0) {
                    console.log('Typeform widgets detected:', widgets.length);
                }
            }
        });
    </script>
