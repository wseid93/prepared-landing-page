    <!-- Non-critical JavaScript - Deferred loading -->
    <script defer>
        // Performance monitoring
        if (window.performance && window.performance.timing) {
            window.addEventListener('load', function() {
                setTimeout(function() {
                    const timing = window.performance.timing;
                    const pageLoadTime = timing.loadEventEnd - timing.navigationStart;
                    const domLoadTime = timing.domContentLoadedEventEnd - timing.navigationStart;
                    
                    console.log('Page Load Time: ' + (pageLoadTime / 1000).toFixed(2) + ' seconds');
                    console.log('DOM Load Time: ' + (domLoadTime / 1000).toFixed(2) + ' seconds');
                    
                    // Add to localStorage for comparison with future visits
                    try {
                        const prevLoadTime = localStorage.getItem('prevPageLoadTime');
                        if (prevLoadTime) {
                            const improvement = ((prevLoadTime - pageLoadTime) / prevLoadTime * 100).toFixed(1);
                            if (improvement > 0) {
                                console.log('Performance improved by ' + improvement + '% since last visit');
                            }
                        }
                        localStorage.setItem('prevPageLoadTime', pageLoadTime);
                    } catch (e) {
                        // Ignore storage errors
                    }
                }, 0);
            });
        }
    </script>
    
    <!-- Animation and interaction functionality - loaded after DOM ready -->
    <script defer>
        document.addEventListener('DOMContentLoaded', () => {
            document.body.classList.add('loaded');
            
            // Check if device is mobile
            const isMobile = window.matchMedia('(max-width: 768px)').matches;
            
            // Exit Intent handling - only add for desktop
            if (!isMobile) {
                let hasShownPopup = false;
                document.addEventListener('mouseleave', (e) => {
                    if (e.clientY < 0 && !hasShownPopup) {
                        document.getElementById('exit-popup').classList.add('visible');
                        document.getElementById('exit-popup-overlay').classList.add('visible');
                        hasShownPopup = true;
                    }
                });

                document.getElementById('exit-popup-overlay')?.addEventListener('click', () => {
                    document.getElementById('exit-popup').classList.remove('visible');
                    document.getElementById('exit-popup-overlay').classList.remove('visible');
                });
            } else {
                // Hide exit popup elements on mobile
                const exitPopup = document.getElementById('exit-popup');
                const exitOverlay = document.getElementById('exit-popup-overlay');
                if (exitPopup) exitPopup.style.display = 'none';
                if (exitOverlay) exitOverlay.style.display = 'none';
            }
            
            // Update spots counter functionality
            const updateSpotsDisplay = (spots) => {
                const spotCounters = document.querySelectorAll('#spots-left, #popup-spots-left');
                spotCounters.forEach(counter => {
                    if (counter) counter.textContent = spots;
                });

                if (spots <= 0) {
                    document.querySelectorAll('.spots-counter').forEach(el => {
                        if (el) el.style.display = 'none';
                    });
                }
            };
            
            let spotsLeft = 85; // Initial spots
            updateSpotsDisplay(spotsLeft);

            // Listen for Typeform submission events
            window.addEventListener('message', (event) => {
                if (event.data.type === 'form-submit') {
                    spotsLeft = Math.max(0, spotsLeft - 1);
                    updateSpotsDisplay(spotsLeft);
                }
            });
        });
    </script>
    
    <!-- Low-priority features - only loaded after page is fully rendered -->
    <script defer>
        window.addEventListener('load', () => {
            // Initialize countdown timer with requestAnimationFrame for better performance
            const updateCountdown = () => {
                const endDate = new Date('2025-04-12T06:59:59Z').getTime();
                
                // Use less frequent updates on mobile
                const isMobile = window.matchMedia('(max-width: 768px)').matches;
                const updateInterval = isMobile ? 10000 : 1000; // 10 seconds on mobile, 1 second on desktop
                
                const updateTimer = () => {
                    const currentTime = new Date().getTime();
                    const difference = endDate - currentTime;
                    
                    const days = Math.floor(difference / (1000 * 60 * 60 * 24));
                    const hours = Math.floor((difference % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60));
                    const minutes = Math.floor((difference % (1000 * 60 * 60)) / (1000 * 60));
                    const seconds = Math.floor((difference % (1000 * 60)) / 1000);
                    
                    const daysElement = document.getElementById('countdown-days');
                    const hoursElement = document.getElementById('countdown-hours');
                    const minutesElement = document.getElementById('countdown-minutes');
                    const secondsElement = document.getElementById('countdown-seconds');
                    
                    if (daysElement) daysElement.textContent = String(days).padStart(2, '0');
                    if (hoursElement) hoursElement.textContent = String(hours).padStart(2, '0');
                    if (minutesElement) minutesElement.textContent = String(minutes).padStart(2, '0');
                    if (secondsElement) secondsElement.textContent = String(seconds).padStart(2, '0');
                    
                    // Use requestAnimationFrame instead of setInterval for better performance
                    if (difference > 0) {
                        // Only update once per second to save CPU on mobile
                        setTimeout(() => requestAnimationFrame(updateTimer), updateInterval);
                    }
                }
                
                updateTimer();
            };
            
            // Initialize countdown if elements exist
            if (document.getElementById('countdown-days')) {
                updateCountdown();
            }
            
            // Detect if device is low-end or mobile
            const isMobile = window.matchMedia('(max-width: 768px)').matches;
            const isLowEndDevice = () => {
                // Check for low memory (less than 2GB)
                if (navigator.deviceMemory && navigator.deviceMemory < 2) return true;
                
                // Check for low-end CPU (older/slower devices)
                if (navigator.hardwareConcurrency && navigator.hardwareConcurrency <= 4) return true;
                
                return false;
            };
            
            // Initialize Intersection Observer for animations only on desktop or high-end devices
            if (!isMobile && !isLowEndDevice() && !window.matchMedia('(prefers-reduced-motion: reduce)').matches) {
                const animationObserver = new IntersectionObserver((entries) => {
                    entries.forEach(entry => {
                        if (entry.isIntersecting) {
                            entry.target.classList.add('animate-visible');
                            
                            // Add card index for staggered animations
                            const cards = entry.target.querySelectorAll('.benefit-card, .testimonial-card');
                            cards.forEach((card, index) => {
                                card.style.setProperty('--card-index', index);
                            });
                        }
                    });
                }, { 
                    root: null, 
                    rootMargin: '0px', 
                    threshold: 0.1  // Lower threshold to improve performance
                });
                
                // Observe all animated sections
                document.querySelectorAll('.section-transition, .testimonials, .trusted-by').forEach((element) => {
                    animationObserver.observe(element);
                });
            } else {
                // For mobile or low-end devices, just show everything without animations
                document.querySelectorAll('.section-transition, .testimonials, .trusted-by').forEach((element) => {
                    element.classList.add('animate-visible');
                });
            }
        });
        
        // Load advanced animations and effects after everything else
        window.addEventListener('load', () => {
            // Wait to ensure all critical content is loaded
            setTimeout(() => {
                // Mobile CTA visibility optimized with passive event listener
                const mobileCTA = document.querySelector('.mobile-cta');
                const isMobile = window.matchMedia('(max-width: 768px)').matches;
                let lastScrollTop = 0;
                let ctaTicking = false;
                
                if (mobileCTA && isMobile) {
                    // Use passive event listener for better scroll performance
                    window.addEventListener('scroll', () => {
                        if (!ctaTicking) {
                            requestAnimationFrame(() => {
                                const scrollTop = window.pageYOffset || document.documentElement.scrollTop;
                                const isScrollingDown = scrollTop > lastScrollTop;
                                const isTopOfPage = scrollTop < 100;

                                if (isScrollingDown && !isTopOfPage) {
                                    mobileCTA.classList.add('mobile-cta-visible');
                                } else {
                                    mobileCTA.classList.remove('mobile-cta-visible');
                                }

                                lastScrollTop = scrollTop;
                                ctaTicking = false;
                            });
                            ctaTicking = true;
                        }
                    }, { passive: true }); // Add passive flag for better performance
                }

                // Parallax scroll effect for hero section (optimized with requestAnimationFrame) - desktop only
                if (!isMobile) {
                    let lastScrollY = window.scrollY;
                    const hero = document.querySelector('.hero');
                    let ticking = false;
                    
                    if (hero) {
                        window.addEventListener('scroll', () => {
                            if (!ticking) {
                                requestAnimationFrame(() => {
                                    const scrollY = window.scrollY;
                                    if (scrollY > 0 && scrollY < window.innerHeight) {
                                        const offset = scrollY * 0.5;
                                        hero.style.setProperty('--scroll-offset', `${offset}px`);
                                        hero.classList.add('scrolling');
                                    } else {
                                        hero.classList.remove('scrolling');
                                    }
                                    lastScrollY = scrollY;
                                    ticking = false;
                                });
                                ticking = true;
                            }
                        }, { passive: true });
                    }
                }
                
                // Smooth scroll for navigation links
                document.querySelectorAll('a[href^="#"]').forEach(anchor => {
                    anchor.addEventListener('click', function (e) {
                        e.preventDefault();
                        const target = document.querySelector(this.getAttribute('href'));
                        if (target) {
                            const headerOffset = 80;
                            const elementPosition = target.getBoundingClientRect().top;
                            const offsetPosition = elementPosition - headerOffset;

                            window.scrollBy({
                                top: offsetPosition,
                                behavior: 'smooth'
                            });
                        }
                    });
                });
            }, 100); // Small delay to ensure critical content is handled first
        }, { once: true }); // Only run once
    </script>
    
    <!-- Lazy loading polyfill -->
    <script defer>
        window.addEventListener('load', function() {
            // Only run if needed
            if ('IntersectionObserver' in window && !('loading' in HTMLImageElement.prototype)) {
                // Create an observer for images
                const imgObserver = new IntersectionObserver((entries, observer) => {
                    entries.forEach(entry => {
                        if (entry.isIntersecting) {
                            const img = entry.target;
                            const dataSrc = img.getAttribute('data-src');
                            if (dataSrc) {
                                img.src = dataSrc;
                                img.removeAttribute('data-src');
                            }
                            observer.unobserve(img);
                        }
                    });
                });

                // Apply to all images with loading="lazy"
                document.querySelectorAll('img[loading="lazy"]').forEach(img => {
                    // Backup the original src
                    if (!img.hasAttribute('data-src')) {
                        img.setAttribute('data-src', img.src);
                        // Use a small placeholder or data URI
                        img.src = 'data:image/svg+xml,%3Csvg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 1 1"%3E%3C/svg%3E';
                        // Observe the image
                        imgObserver.observe(img);
                    }
                });
            }
        }, { once: true }); // Only run once
    </script>
    
