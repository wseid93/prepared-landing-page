    <!-- Google Analytics 4 with Enhanced Measurement - Optimized loading -->
    <script>
      window.addEventListener('DOMContentLoaded', function() {
        var script = document.createElement('script');
        script.src = 'https://www.googletagmanager.com/gtag/js?id=G-XXXXXXXXXX';
        script.async = true;
        document.head.appendChild(script);
        
        window.dataLayer = window.dataLayer || [];
        function gtag(){dataLayer.push(arguments);}
        gtag('js', new Date());
        
        // Configure your GA4 property with enhanced measurement
        gtag('config', 'G-XXXXXXXXXX', {
          'send_page_view': true,
          'cookie_domain': 'eatprepared.com',
          'cookie_flags': 'SameSite=None;Secure',
          'custom_map': {
            'dimension1': 'client_id',
            'dimension2': 'user_agent',
            'dimension3': 'page_load_time'
          }
        });
      });
      
      // Track Core Web Vitals
      window.addEventListener('load', function() {
        setTimeout(function() {
          if (window.performance) {
            const pageNav = performance.getEntriesByType("navigation")[0];
            if (pageNav) {
              const loadTime = Math.round(pageNav.loadEventStart - pageNav.startTime);
              gtag('event', 'page_load_performance', {
                'page_load_time': loadTime,
                'dom_interactive_time': Math.round(pageNav.domInteractive - pageNav.startTime),
                'content_load_time': Math.round(pageNav.domContentLoadedEventEnd - pageNav.startTime)
              });
            }
          }
        }, 0);
      });
    </script>

