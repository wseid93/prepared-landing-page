    <!-- Critical CSS inlined -->
    <style>
        /* Critical CSS for above-the-fold content */
        * {margin: 0; padding: 0; box-sizing: border-box;}
        :root {
            --accent: #22C55E;
            --font-primary: 'Inter', -apple-system, BlinkMacSystemFont, 'SF Pro Display', 'Segoe UI', Roboto, sans-serif;
        }
        body {
            font-family: var(--font-primary);
            font-size: 16px;
            line-height: 1.5;
            color: #1A202C;
            background-color: #FFFFFF;
            opacity: 1;
            transition: opacity 0.3s ease;
            -webkit-text-size-adjust: 100%; /* Prevent iOS font size adjustments */
        }
        body.loaded {opacity: 1;}
        .hero {
            min-height: 85vh;
            display: flex;
            align-items: center;
            justify-content: center;
            color: #fff;
            position: relative;
            overflow: hidden;
            background: #000;
            padding-top: 80px;
            padding-bottom: 30px;
            margin-bottom: -50px;
            contain: layout paint; /* Optimize rendering performance */
        }
        .hero-content {
            max-width: 1000px;
            margin: 0 auto;
            position: relative;
            z-index: 2;
            text-align: center;
            padding: 0 2rem;
            will-change: transform; /* Hint for browser optimization */
        }
        @media (max-width: 768px) {
            .hero {
                min-height: 80vh;
                contain: layout paint; /* Optimize rendering on mobile */
            }
            .hero-video {
                display: none;
            }
            .hero-image-container {
                background-image: url('https://i.imgur.com/lhhxgxB.avif');
                background-image: -webkit-image-set(
                    url('https://i.imgur.com/lhhxgxB.avif') type("image/avif"),
                    url('https://i.imgur.com/lhhxgxB.webp') type("image/webp"),
                    url('https://i.imgur.com/lhhxgxB.jpg') 1x
                );
                background-image: image-set(
                    url('https://i.imgur.com/lhhxgxB.avif') type("image/avif"),
                    url('https://i.imgur.com/lhhxgxB.webp') type("image/webp"),
                    url('https://i.imgur.com/lhhxgxB.jpg') 1x
                );
                background-size: cover;
                background-position: center;
                position: absolute;
                top: 0;
                left: 0;
                width: 100%;
                height: 100%;
                z-index: 1;
                contain: paint; /* Less restrictive containment for better layout */
            }
            .navbar {
                display: flex;
                justify-content: space-between;
                padding: 10px 20px;
                background-color: #000;
                position: fixed;
                top: 0;
                left: 0;
                right: 0;
                z-index: 1000;
            }
            .navbar .nav-logo {font-size: 1.2rem;}
            .navbar .nav-container {
                flex-direction: column;
                align-items: center;
            }
            /* Optimize tap targets for mobile */
            button, 
            [role="button"],
            a {
                min-height: 44px;
                min-width: 44px;
                display: inline-flex;
                align-items: center;
                justify-content: center;
            }
            /* Prevent content reflow */
            img, video {
                height: auto;
                aspect-ratio: attr(width) / attr(height);
                max-width: 100%;
                display: block;
            }
            /* Add content-visibility for offscreen content */
            .section-transition:not(.hero) {
                content-visibility: auto;
                contain-intrinsic-size: 1px 500px;
            }
            /* Add display: contents to parent containers of images to prevent CLS */
            .partner-image, .share-card > img {
                width: 100%;
                height: auto;
                display: block;
            }
            
            /* Mobile-specific styles for better readability */
            h1 {
                font-size: 2.5rem;
                line-height: 1.2;
            }
            h2 {
                font-size: 2rem;
                line-height: 1.25;
            }
            h3 {
                font-size: 1.5rem;
                line-height: 1.3;
            }
            p {
                font-size: 1rem;
                line-height: 1.5;
            }
            .hero-stats {
                flex-direction: column;
                gap: 1rem;
            }
            .two-column-grid {
                grid-template-columns: 1fr;
            }
            .share-grid, .benefits-grid, .stats-grid, .faq-grid {
                grid-template-columns: 1fr;
                gap: 1.5rem;
            }
            .footer-content {
                flex-direction: column;
                gap: 2rem;
                text-align: center;
            }
            .social-links {
                justify-content: center;
            }
        }
        
        /* Medium screens */
        @media (min-width: 769px) and (max-width: 1024px) {
            .hero-content {
                max-width: 800px;
            }
            .share-grid, .benefits-grid {
                grid-template-columns: repeat(2, 1fr);
            }
            h1 {
                font-size: 3rem;
            }
            h2 {
                font-size: 2.25rem;
            }
        }
        
        /* Large screens */
        @media (min-width: 1025px) {
            .hero-content {
                max-width: 1000px;
            }
            h1 {
                font-size: 3.5rem;
            }
            button, 
            [role="button"],
            a {
                transition: transform 0.2s ease, box-shadow 0.2s ease;
            }
            button:hover, 
            [role="button"]:hover,
            a:hover {
                transform: translateY(-2px);
            }
        }
        
        /* Stats section icon styling */
        .stat-icon {
            display: flex;
            align-items: center;
            justify-content: center;
            width: 60px;
            height: 60px;
            background-color: rgba(34, 197, 94, 0.3);
            border-radius: 12px;
            margin-right: 15px;
            box-shadow: 0 0 15px rgba(34, 197, 94, 0.5);
            transition: all 0.3s ease;
        }
        
        .stat-icon svg {
            width: 36px;
            height: 36px;
            filter: drop-shadow(0 0 5px rgba(45, 218, 107, 0.9));
        }
        
        .stat-card:hover .stat-icon {
            background-color: rgba(34, 197, 94, 0.4);
            box-shadow: 0 0 25px rgba(34, 197, 94, 0.8);
            transform: scale(1.05);
        }
        
        /* Print styles for accessibility */
        @media print {
            body {
                background: white;
                color: black;
                font-size: 12pt;
            }
            .hero {
                height: auto;
                min-height: auto;
                background: white;
                color: black;
            }
            .hero-video, .hero-image-container, .exit-popup, .exit-popup-overlay, .mobile-cta, .navbar, [data-tf-live] {
                display: none !important;
            }
            a {
                color: #000;
                text-decoration: underline;
            }
            a[href]:after {
                content: " (" attr(href) ")";
            }
            .hero-content, .share-section, .partners-showcase, .benefits, .stats, .faq, footer {
                page-break-inside: avoid;
            }
        }
    </style>

